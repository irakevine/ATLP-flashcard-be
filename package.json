{
  "name": "flashcard-be",
  "version": "1.0.0",
  "description": "flashcard Project",
  "main": "index.js",
  "scripts": {
    "postinstall": "npm -s run generate",
    "dev": "NODE_ENV=development ts-node-dev -r dotenv/config --no-notify --respawn --transpile-only src",

  "devDependencies": {
    "ts-node-dev": "^1.1.8",
    "typescript": "^5.1.6"
  },
  "dependencies": {
    "apollo-server-core": "^3.3.0",
    "apollo-server-express": "^3.3.0",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "graphql": "^15.5.3"
  }
}
